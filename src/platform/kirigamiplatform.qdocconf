include($KDE_DOCS/global/qt-module-defaults.qdocconf)

project = KirigamiPlatform
description = Platform Integration Components

documentationinheaders = true

headerdirs += .
sourcedirs += .

depends = kde qtcore qtquick qtquickcontrols

navigation.landingpage = "Kirigami"

qhp.projects            = KirigamiPlatform

qhp.KirigamiPlatform.file          = kirigamiplatform.qhp
qhp.KirigamiPlatform.namespace     = org.kde.kirigamiplatform.$QT_VERSION_TAG
qhp.KirigamiPlatform.virtualFolder = kirigamiplatform
qhp.KirigamiPlatform.indexTitle    = Kirigami Platform
qhp.KirigamiPlatform.indexRoot     =

qhp.KirigamiPlatform.subprojects = qmltypes classes

qhp.KirigamiPlatform.subprojects.qmltypes.title = Kirigami Platform QML Types
qhp.KirigamiPlatform.subprojects.qmltypes.indexTitle = Kirigami Platform QML Types
qhp.KirigamiPlatform.subprojects.qmltypes.selectors = qmlclass
qhp.KirigamiPlatform.subprojects.qmltypes.sortPages = true

qhp.KirigamiPlatform.subprojects.classes.title = Kirigami Platform C++ Classes
qhp.KirigamiPlatform.subprojects.classes.indexTitle = Kirigami Platform C++ Classes
qhp.KirigamiPlatform.subprojects.classes.selectors = class fake:headerfile
qhp.KirigamiPlatform.subprojects.classes.sortPages = true

tagfile = kirigamiplatform.tags
